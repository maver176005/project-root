required>\n        <label for=\"model\">Модель:</label>\n        <input v-model=\"newCar.model\" id=\"model\" required>\n        <button type=\"submit\">Добавить</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { ref } from 'vue';\n\nexport default {\n  name: 'Cars',\n  setup() {\n    const cars = ref([]);\n    const showForm = ref(false);\n    const newCar = ref({ brand: '', model: '' });\n\n    const fetchCars = async () => {\n      try {\n        const response = await axios.get('/api/cars');\n        cars.value = response.data;\n      } catch (error) {\n        console.error('Не удалось получить данные об автомобилях', error);\n      }\n    };\n\n    const showAddCarForm = () => {\n      showForm.value = true;\n    };\n\n    const addCar = async () => {\n      try {\n        await axios.post('/api/cars', newCar.value);\n        fetchCars();\n        newCar.value = { brand: '', model: '' };\n        showForm.value = false;\n      } catch (error) {\n        console.error('Не удалось добавить автомобиль', error);\n      }\n    };\n\n    fetchCars();\n\n    return {\n      cars,\n      showForm,\n      newCar,\n      showAddCarForm,\n      addCar,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.glassmorphism {\n  background: rgba(255, 255, 255, 0.1);\n  backdrop-filter: blur(10px);\n  -webkit-backdrop-filter: blur(10px);\n  border-radius: 10px;\n  box-shadow: inset 0 0 10px rgba(0, 0, 0, 0.2);\n  padding: 20px;\n  margin: 20px;\n}\nbutton {\n  margin-top: 10px;\n  background: rgba(255, 255, 255, 0.1);\n  border: none;\n  padding: 10px 20px;\n  border-radius: 10px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  cursor: pointer;\n}\nform {\n  margin-top: 20px;\n}\nlabel {\n  display: block;\n  margin: 10px 0 5px;\n}\ninput {\n  width: calc(100% - 20px);\n  padding: 8px;\n  border-radius: 5px;\n  border: 1px solid rgba(0, 0, 0, 0.1);\n}\ntable {\n  width: 100%;\n  border-collapse: collapse;\n  margin-top: 20px;\n}\nth, td {\n  border: 1px solid rgba(0, 0, 0, 0.1);\n  padding: 10px;\n  text-align: left;\n}\n</style>\n"],"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIH,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMI,QAAO,GAAIJ,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAMK,MAAK,GAAIL,GAAG,CAAC;MAAEM,KAAK,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAG,CAAC,CAAC;IAE5C,MAAMC,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,WAAW,CAAC;QAC7CP,IAAI,CAACQ,KAAI,GAAIF,QAAQ,CAACG,IAAI;MAC5B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2CAA2C,EAAEA,KAAK,CAAC;MACnE;IACF,CAAC;IAED,MAAME,cAAa,GAAIA,CAAA,KAAM;MAC3BX,QAAQ,CAACO,KAAI,GAAI,IAAI;IACvB,CAAC;IAED,MAAMK,MAAK,GAAI,MAAAA,CAAA,KAAY;MACzB,IAAI;QACF,MAAMjB,KAAK,CAACkB,IAAI,CAAC,WAAW,EAAEZ,MAAM,CAACM,KAAK,CAAC;QAC3CH,SAAS,CAAC,CAAC;QACXH,MAAM,CAACM,KAAI,GAAI;UAAEL,KAAK,EAAE,EAAE;UAAEC,KAAK,EAAE;QAAG,CAAC;QACvCH,QAAQ,CAACO,KAAI,GAAI,KAAK;MACxB,EAAE,OAAOE,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD;IACF,CAAC;IAEDL,SAAS,CAAC,CAAC;IAEX,OAAO;MACLL,IAAI;MACJC,QAAQ;MACRC,MAAM;MACNU,cAAc;MACdC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}