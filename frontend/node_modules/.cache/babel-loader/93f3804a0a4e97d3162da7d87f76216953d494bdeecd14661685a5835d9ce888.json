{"ast":null,"code":"import axios from 'axios';\nimport { ref, reactive, onMounted } from 'vue';\nexport default {\n  name: 'Stores',\n  setup() {\n    const stores = ref([]);\n    const newStore = reactive({\n      name: '',\n      phone: ''\n    });\n    const showAddStore = ref(false);\n    const showAttachCar = ref(false);\n    const carId = ref(null);\n    const currentStoreId = ref(null);\n    const fetchStores = async () => {\n      try {\n        const response = await axios.get('/api/stores');\n        stores.value = response.data;\n      } catch (error) {\n        console.error('Не удалось получить данные о магазинах', error);\n      }\n    };\n    const addStore = async () => {\n      try {\n        const response = await axios.post('http://localhost:8085/api/stores', {\n          name: newStore.name,\n          phone: newStore.phone\n        });\n        console.log('Store added:', response.data);\n        await fetchStores();\n        newStore.name = '';\n        newStore.phone = '';\n        showAddStore.value = false;\n      } catch (error) {\n        console.error('Не удалось добавить магазин', error);\n      }\n    };\n    const attachCar = async () => {\n      try {\n        await axios.post('http://localhost:8080/api/stores/attach-car', {\n          store_id: currentStoreId.value,\n          car_id: carId.value\n        });\n        console.log('Car attached');\n        await fetchStores();\n        carId.value = null;\n        showAttachCar.value = false;\n      } catch (error) {\n        console.error('Не удалось прикрепить автомобиль к магазину', error);\n      }\n    };\n    const showAddStoreForm = () => {\n      showAddStore.value = true;\n      showAttachCar.value = false;\n    };\n    const showAttachCarForm = storeId => {\n      currentStoreId.value = storeId;\n      showAttachCar.value = true;\n      showAddStore.value = false;\n    };\n    onMounted(() => {\n      fetchStores();\n    });\n    return {\n      stores,\n      newStore,\n      showAddStore,\n      showAttachCar,\n      carId,\n      addStore,\n      attachCar,\n      showAddStoreForm,\n      showAttachCarForm\n    };\n  }\n};","map":{"version":3,"names":["axios","ref","reactive","onMounted","name","setup","stores","newStore","phone","showAddStore","showAttachCar","carId","currentStoreId","fetchStores","response","get","value","data","error","console","addStore","post","log","attachCar","store_id","car_id","showAddStoreForm","showAttachCarForm","storeId"],"sources":["/Users/zeus_olimp/WORK/project-root/frontend/src/views/Stores.vue"],"sourcesContent":["<template>\n  <div class=\"glass-container\">\n    <h1>Stores</h1>\n    <table class=\"glassmorphism\">\n      <thead>\n      <tr>\n        <th>Name</th>\n        <th>Phone</th>\n      </tr>\n      </thead>\n      <tbody>\n      <tr v-for=\"store in stores\" :key=\"store.id\">\n        <td>{{ store.name }}</td>\n        <td>{{ store.phone }}</td>\n        <td><button @click=\"showAttachCarForm(store.id)\">Attach Car</button></td>\n      </tr>\n      </tbody>\n    </table>\n\n    <button @click=\"showAddStoreForm\">Add Store</button>\n\n    <div v-if=\"showAddStore\">\n      <h2>Add a New Store</h2>\n      <form @submit.prevent=\"addStore\">\n        <div>\n          <label for=\"name\">Name:</label>\n          <input type=\"text\" id=\"name\" v-model=\"newStore.name\" required>\n        </div>\n        <div>\n          <label for=\"phone\">Phone:</label>\n          <input type=\"text\" id=\"phone\" v-model=\"newStore.phone\" required>\n        </div>\n        <button type=\"submit\">Add Store</button>\n      </form>\n    </div>\n\n    <div v-if=\"showAttachCar\">\n      <h2>Attach Car to Store</h2>\n      <form @submit.prevent=\"attachCar\">\n        <div>\n          <label for=\"car_id\">Car ID:</label>\n          <input type=\"number\" id=\"car_id\" v-model=\"carId\" required>\n        </div>\n        <button type=\"submit\">Attach Car</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { ref, reactive, onMounted } from 'vue';\n\nexport default {\n  name: 'Stores',\n  setup() {\n    const stores = ref([]);\n    const newStore = reactive({\n      name: '',\n      phone: ''\n    });\n    const showAddStore = ref(false);\n    const showAttachCar = ref(false);\n    const carId = ref(null);\n    const currentStoreId = ref(null);\n\n    const fetchStores = async () => {\n      try {\n        const response = await axios.get('/api/stores');\n        stores.value = response.data;\n      } catch (error) {\n        console.error('Не удалось получить данные о магазинах', error);\n      }\n    };\n\n    const addStore = async () => {\n      try {\n        const response = await axios.post('http://localhost:8085/api/stores', {\n          name: newStore.name,\n          phone: newStore.phone,\n        });\n        console.log('Store added:', response.data);\n        await fetchStores();\n        newStore.name = '';\n        newStore.phone = '';\n        showAddStore.value = false;\n      } catch (error) {\n        console.error('Не удалось добавить магазин', error);\n      }\n    };\n\n    const attachCar = async () => {\n      try {\n        await axios.post('http://localhost:8080/api/stores/attach-car', {\n          store_id: currentStoreId.value,\n          car_id: carId.value,\n        });\n        console.log('Car attached');\n        await fetchStores();\n        carId.value = null;\n        showAttachCar.value = false;\n      } catch (error) {\n        console.error('Не удалось прикрепить автомобиль к магазину', error);\n      }\n    };\n\n    const showAddStoreForm = () => {\n      showAddStore.value = true;\n      showAttachCar.value = false\n\n    };\n\n    const showAttachCarForm = (storeId) => {\n      currentStoreId.value = storeId;\n      showAttachCar.value = true;\n      showAddStore.value = false;\n    };\n\n    onMounted(() => {\n      fetchStores();\n    });\n\n    return {\n      stores,\n      newStore,\n      showAddStore,\n      showAttachCar,\n      carId,\n      addStore,\n      attachCar,\n      showAddStoreForm,\n      showAttachCarForm,\n    };\n  }\n};\n</script>\n\n<style scoped>\n.glass-container {\n  margin: 10px;\n  background: rgba(210, 217, 217, 0.6);\n  border-radius: 10px;\n  padding: 20px;\n  backdrop-filter: blur(10px);\n  box-shadow: 0 4px 6px rgb(100, 96, 96);\n  border: 1px solid rgba(255, 255, 255, 0.3);\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n  border-radius: 2%;\n}\n\nth, td {\n  border: 1px solid rgba(0, 0, 0, 0.43);\n  padding: 8px;\n  text-align: left;\n}\n\nth div {\n  cursor: col-resize;\n  display: inline-block;\n  width: 100%;\n}\n\nform {\n  margin-top: 20px;\n}\n\nform div {\n  margin-bottom: 10px;\n}\n\nform label {\n  display: inline-block;\n  width: 100px;\n  text-align: right;\n  margin-right: 10px;\n}\n\nform input {\n  padding: 5px;\n  width: 200px;\n}\n\nbutton {\n  padding: 10px 20px;\n  background-color: #42b983;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\nbutton:hover {\n  background-color: #369f77;\n}\n</style>\n"],"mappings":"AAkDA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AAE9C,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIL,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMM,QAAO,GAAIL,QAAQ,CAAC;MACxBE,IAAI,EAAE,EAAE;MACRI,KAAK,EAAE;IACT,CAAC,CAAC;IACF,MAAMC,YAAW,GAAIR,GAAG,CAAC,KAAK,CAAC;IAC/B,MAAMS,aAAY,GAAIT,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMU,KAAI,GAAIV,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMW,cAAa,GAAIX,GAAG,CAAC,IAAI,CAAC;IAEhC,MAAMY,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,aAAa,CAAC;QAC/CT,MAAM,CAACU,KAAI,GAAIF,QAAQ,CAACG,IAAI;MAC9B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wCAAwC,EAAEA,KAAK,CAAC;MAChE;IACF,CAAC;IAED,MAAME,QAAO,GAAI,MAAAA,CAAA,KAAY;MAC3B,IAAI;QACF,MAAMN,QAAO,GAAI,MAAMd,KAAK,CAACqB,IAAI,CAAC,kCAAkC,EAAE;UACpEjB,IAAI,EAAEG,QAAQ,CAACH,IAAI;UACnBI,KAAK,EAAE