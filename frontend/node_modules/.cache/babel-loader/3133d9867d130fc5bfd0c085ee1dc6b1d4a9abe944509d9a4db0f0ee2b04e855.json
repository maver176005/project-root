{"ast":null,"code":"import axios from 'axios';\nimport { ref } from 'vue';\nexport default {\n  name: 'Stores',\n  setup() {\n    const stores = ref([]);\n    const showForm = ref(false);\n    const newStore = ref({\n      name: '',\n      phone: ''\n    });\n    const API_BASE_URL = 'http://localhost:8085/api';\n    const fetchStores = async () => {\n      try {\n        const response = await axios.get(`${API_BASE_URL}/stores`);\n        stores.value = response.data;\n      } catch (error) {\n        console.error('Не удалось получить данные о магазинах', error);\n      }\n    };\n    const showAddStoreForm = () => {\n      showForm.value = true;\n    };\n    const addStore = async () => {\n      try {\n        await axios.post(`${API_BASE_URL}/stores`, newStore.value);\n        await fetchStores();\n        newStore.value = {\n          name: '',\n          phone: ''\n        };\n        showForm.value = false;\n      } catch (error) {\n        console.error('Не удалось добавить магазин', error);\n      }\n    };\n    fetchStores();\n    return {\n      stores,\n      showForm,\n      newStore,\n      showAddStoreForm,\n      addStore\n    };\n  }\n};","map":{"version":3,"names":["axios","ref","name","setup","stores","showForm","newStore","phone","API_BASE_URL","fetchStores","response","get","value","data","error","console","showAddStoreForm","addStore","post"],"sources":["/Users/zeus_olimp/WORK/project-root/frontend/src/views/Stores.vue"],"sourcesContent":["<template>\n  <div class=\"glassmorphism\">\n    <h1>Магазины</h1>\n    <ul>\n      <li v-for=\"store in stores\" :key=\"store.id\">\n        {{ store.name }} - {{ store.phone }}\n      </li>\n    </ul>\n    <button @click=\"showAddStoreForm\">Добавить магазин</button>\n    <div v-if=\"showForm\">\n      <h2>Добавить новый магазин</h2>\n      <form @submit.prevent=\"addStore\">\n        <label for=\"name\">Название:</label>\n        <input v-model=\"newStore.name\" id=\"name\" required>\n        <label for=\"phone\">Телефон:</label>\n        <input v-model=\"newStore.phone\" id=\"phone\" required>\n        <button type=\"submit\">Добавить</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { ref } from 'vue';\n\nexport default {\n  name: 'Stores',\n  setup() {\n    const stores = ref([]);\n    const showForm = ref(false);\n    const newStore = ref({ name: '', phone: '' });\n    const API_BASE_URL = 'http://localhost:8085/api';\n\n    const fetchStores = async () => {\n      try {\n        const response = await axios.get(`${API_BASE_URL}/stores`);\n        stores.value = response.data;\n      } catch (error) {\n        console.error('Не удалось получить данные о магазинах', error);\n      }\n    };\n\n    const showAddStoreForm = () => {\n      showForm.value = true;\n    };\n\n    const addStore = async () => {\n      try {\n        await axios.post(`${API_BASE_URL}/stores`, newStore.value);\n        await fetchStores();\n        newStore.value = { name: '', phone: '' };\n        showForm.value = false;\n      } catch (error) {\n        console.error('Не удалось добавить магазин', error);\n      }\n    };\n\n    fetchStores();\n\n    return {\n      stores,\n      showForm,\n      newStore,\n      showAddStoreForm,\n      addStore,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.glassmorphism {\n  background: rgba(255, 255, 255, 0.1);\n  backdrop-filter: blur(10px);\n  -webkit-backdrop-filter: blur(10px);\n  border-radius: 10px;\n  box-shadow: inset 0 0 10px rgba(0, 0, 0, 0.2);\n  padding: 20px;\n  margin: 20px;\n}\nbutton {\n  margin-top: 10px;\n  background: rgba(255, 255, 255, 0.1);\n  border: none;\n  padding: 10px 20px;\n  border-radius: 10px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  cursor: pointer;\n}\nform {\n  margin-top: 20px;\n}\nlabel {\n  display: block;\n  margin: 10px 0 5px;\n}\ninput {\n  width: calc(100% - 20px);\n  padding: 8px;\n  border-radius: 5px;\n  border: 1px solid rgba(0, 0, 0, 0.1);\n}\n</style>\n"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIH,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMI,QAAO,GAAIJ,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAMK,QAAO,GAAIL,GAAG,CAAC;MAAEC,IAAI,EAAE,EAAE;MAAEK,KAAK,EAAE;IAAG,CAAC,CAAC;IAC7C,MAAMC,YAAW,GAAI,2BAA2B;IAEhD,MAAMC,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,GAAGH,YAAY,SAAS,CAAC;QAC1DJ,MAAM,CAACQ,KAAI,GAAIF,QAAQ,CAACG,IAAI;MAC9B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wCAAwC,EAAEA,KAAK,CAAC;MAChE;IACF,CAAC;IAED,MAAME,gBAAe,GAAIA,CAAA,KAAM;MAC7BX,QAAQ,CAACO,KAAI,GAAI,IAAI;IACvB,CAAC;IAED,MAAMK,QAAO,GAAI,MAAAA,CAAA,KAAY;MAC3B,IAAI;QACF,MAAMjB,KAAK,CAACkB,IAAI,CAAC,GAAGV,YAAY,SAAS,EAAEF,QAAQ,CAACM,KAAK,CAAC;QAC1D,MAAMH,WAAW,CAAC,CAAC;QACnBH,QAAQ,CAACM,KAAI,GAAI;UAAEV,IAAI,EAAE,EAAE;UAAEK,KAAK,EAAE;QAAG,CAAC;QACxCF,QAAQ,CAACO,KAAI,GAAI,KAAK;MACxB,EAAE,OAAOE,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;MACrD;IACF,CAAC;IAEDL,WAAW,CAAC,CAAC;IAEb,OAAO;MACLL,MAAM;MACNC,QAAQ;MACRC,QAAQ;MACRU,gBAAgB;MAChBC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}